{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "max-line-length": [true, {"limit": 120, "ignore-pattern": "'.*'"}],
    "member-ordering": false,
    "no-magic-numbers": true,
    "no-unused-variable": true,
    "quotemark": [true, "single", "jsx-single", "avoid-escape"]
  },
  "rulesDirectory": []
}
